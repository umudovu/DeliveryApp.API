<head>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
<meta name="description" content="Gurdeep Osahan">
<meta name="author" content="Gurdeep Osahan">
<link rel="icon" type="image/png" href="img/logo_web_red.png">
<title>Foodride - Register</title>

<link rel="stylesheet" type="text/css" href="~/vendor/slick/slick.min.css" />
<link rel="stylesheet" type="text/css" href="~/vendor/slick/slick-theme.min.css" />

<link href="~/vendor/icons/feather.css" rel="stylesheet" type="text/css">

<link href="~/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet">

<link href="~/css/style.css" rel="stylesheet">

<link href="~/vendor/sidebar/demo.css" rel="stylesheet">
</head>
<body>
<div class="osahan-signup login-page">
<video loop autoplay muted id="vid">
<source src="~/img/bg.mp4" type="video/mp4">
<source src="~/img/bg.mp4" type="video/ogg">
Your browser does not support the video tag.
</video>
<div class="d-flex align-items-center justify-content-center flex-column vh-100">
<div class="px-5 col-md-6 ml-auto">
<div class="px-5 col-10 mx-auto">
<h2 class="text-dark my-0">Hello There.</h2>
<p class="text-50">Sign up to continue</p>
<form class="mt-5 mb-4" method="post" enctype="multipart/form-data">
    <span asp-validation-for="Name" class="text-danger"></span>
    <span asp-validation-for="SurName" class="text-danger"></span>
<div class="form-group">
<label asp-for="Name" class="text-dark">Name</label>
<div class="row">
    <div class="col-6">
        <input asp-for="Name" placeholder="First Name" class="form-control mb-1"  aria-describedby="nameHelp">
    </div>
    <div class="col-6">
<input asp-for="SurName" placeholder="Last Name" class="form-control"  aria-describedby="nameHelp">
    </div>

</div>
</div>

<div class="form-group">
<div class="row">
    <div class="col-6">
        <span asp-validation-for="PhoneNumber" class="text-danger"></span>
       <label asp-for="PhoneNumber" class="text-dark">Mobile Number</label>
<input  placeholder="Enter Mobile" class="form-control" asp-for="PhoneNumber" >
    </div>
    <div class="col-6">
        <span asp-validation-for="Email" class="text-danger"></span>
        <label asp-for="Email" class="text-dark">E-mail</label>
<input  placeholder="Enter email" class="form-control" asp-for="Email" >
    </div>
</div>
</div>


<span asp-validation-for="Address" class="text-danger"></span>
<div class="form-group">
<label asp-for="Address" class="text-dark">Address</label>
<input  placeholder="Enter addess" class="form-control" asp-for="Address" >
</div>

<div class="form-group">
<label asp-for="Password" class="text-dark">Password</label>
<div class="row">
    <div class="col-6">
        <span asp-validation-for="Password" class="text-danger"></span>
        <input  placeholder="Password" class="form-control" asp-for="Password">
    </div>
    <div class="col-6">
        <span asp-validation-for="PasswordConfirm" class="text-danger"></span>
        <input  placeholder="Confirm Password" class="form-control" asp-for="PasswordConfirm">

    </div>
</div>
</div>


<button type="submit" class="btn btn-primary btn-lg btn-block">
SIGN UP
</button>
@*<div class="py-2">
<button class="btn btn-facebook btn-lg btn-block"><i class="feather-facebook"></i> Connect with Facebook</button>
</div>*@
</form>
</div>
<div class="new-acc d-flex align-items-center justify-content-center">
<a asp-action="login">
<p class="text-center m-0">Already an account? Sign in</p>
</a>
</div>
</div>
</div>
</div>

<script type="c08a8985e095fe6c7772bbf8-text/javascript" src="vendor/jquery/jquery.min.js"></script>
<script type="c08a8985e095fe6c7772bbf8-text/javascript" src="vendor/bootstrap/js/bootstrap.bundle.min.js"></script>

<script type="c08a8985e095fe6c7772bbf8-text/javascript" src="vendor/slick/slick.min.js"></script>

<script type="c08a8985e095fe6c7772bbf8-text/javascript" src="vendor/sidebar/hc-offcanvas-nav.js"></script>

<script type="c08a8985e095fe6c7772bbf8-text/javascript" src="js/osahan.js"></script>
 <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.19.5/jquery.validate.min.js"></script>
    <!-- Asp Jquery Validation cdn-->
    <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validation-unobtrusive/3.2.12/jquery.validate.unobtrusive.min.js"></script>
</body>

</html>